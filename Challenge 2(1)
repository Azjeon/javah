infcrement

package loops;
public class NumbersWhileLoop {
    public static void main(String[] args) {
        int num = 143; 

       
        while (num <= 220) {
            System.out.println(num);
            num++;
        }
    }
}



decrement

public class WhileLoopDecrement {
    public static void main(String[] args) {
        int number = 220;
        while (number >= 143) {
            System.out.print(number + " ");
            number--;
        }
    }
}
