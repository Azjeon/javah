infcrement

public class WhileLoopIncrement {
    public static void main(String[] args) {
        int number = 143;
        while (number <= 220) {
            System.out.print(number + " ");
            number++;
        }
    }
}


decrement

public class WhileLoopDecrement {
    public static void main(String[] args) {
        int number = 220;
        while (number >= 143) {
            System.out.print(number + " ");
            number--;
        }
    }
}
